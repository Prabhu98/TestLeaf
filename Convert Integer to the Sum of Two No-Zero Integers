class Solution {
    public int[] getNoZeroIntegers(int n) {
        
        
        int sum=0;
        
        for(int i =1 ;i<=n;i++){
            
            sum += i;
            
        
            if(sum == n){
                 return new int[]{i,sum};
                 
             }
        }
        
        return new int[]{};
        
    }
    
    

    
    
}
